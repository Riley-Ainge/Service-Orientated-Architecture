<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFClient"
        xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="WPFClient.LoggedIn"
        mc:Ignorable="d"
        Title="LoggedIn" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="Box" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="133,40,206,0" SelectionChanged="Box_SelectionChanged_1"/>
        <TextBox x:Name="Search" HorizontalAlignment="Left" Margin="223,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Search" HorizontalAlignment="Left" Margin="173,6,0,0" VerticalAlignment="Top"/>
        <Button x:Name="SearchBtn" Content="Search" HorizontalAlignment="Left" Margin="348,9,0,0" VerticalAlignment="Top" Click="SearchBtn_Click"/>
        <ProgressBar x:Name="Bar" HorizontalAlignment="Left" Height="10" Margin="318,232,0,0" VerticalAlignment="Top" Width="100" Visibility="Hidden"/>
    </Grid>
</Window>
